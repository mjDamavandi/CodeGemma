{
  "name": "mjdamavandiCodeGemma",
  "displayName": "Code Gemma",
  "description": "an extension to use codegemma on huggingfaces servers for better coding.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "CodeGemma.complete",
      "title": "Complete code with CodeGemma"
    }],
    "configuration": {
      "title": "Code Gemma",
      "properties": {
        "mjdamavandiCodeGemma.AccessTokens": {
          "type": ["string", "null"],
          "default": null,
          "description": "Get your access token from huggingface with the necessary permissions and place it here."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
